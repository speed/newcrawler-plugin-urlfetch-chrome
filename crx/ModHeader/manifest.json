{
  "name": "ModHeader",
  "version": "2.1.1",
  "manifest_version": 2,
  "description": "Modify request and response headers",
  "background":  {
  	"page":"background.html"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Modify Headers"
  },
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "incognito": "spanning",
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ]
}